#include<bits/stdc++.h>
#define ll long long int
#define vvi vector< vector<int> >
#define pb push_back
#define fori(i,n) for(int i=0;i<n;i++)


using namespace std;
int main()
{  
   
  ios_base::sync_with_stdio(false);
  cin.tie(NULL);
  int t;
  cin>>t;
  while(t--)
  {
     int n;
     cin>>n;
     vector<int>arr;
     arr.pb(1);
     int carry=0;
     for(int i=2;i<=n;i++)
     {
         for(int j=0;j<arr.size();j++)
         {
             int mul=arr[j]*i+carry;
             arr[j]=mul%10;
             carry=mul/10;
         }
         while(carry>0)
         {
             arr.pb(carry%10);
             carry=carry/10;
         }
     }
     for(auto it=arr.rbegin();it!=arr.rend();it++)
       cout<<*it;
     cout<<endl;
  }
  //Akash singh parihar
  return 0;
 }
